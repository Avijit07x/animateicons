{
	"name": "animateicons",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@10.0.0",
	"scripts": {
		"format": "prettier --write .",
		"gen:icons-json": "tsx scripts/generate-icons-json.ts",
		"gen:registry": "tsx scripts/generate-registry.ts",
		"gen:icons": "pnpm run gen:icons-json && pnpm run gen:registry",
		"check:icons": "tsx scripts/check-duplicate-icons.ts",
		"check:registry": "tsx scripts/check-registry.ts",
		"check:all": "pnpm run format && pnpm run check:icons && pnpm run check:registry",
		"prebuild": "pnpm run gen:icons && pnpm run check:all",
		"dev": "pnpm run gen:icons && pnpm run check:all && next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint"
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tabs": "^1.1.12",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@radix-ui/react-use-controllable-state": "^1.2.2",
		"@vercel/analytics": "^1.5.0",
		"babel-plugin-react-compiler": "^1.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"fuse.js": "^7.1.0",
		"lucide-react": "^0.533.0",
		"motion": "^12.29.2",
		"next": "^16.1.6",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"tailwind-merge": "^3.3.1",
		"use-debounce": "^10.0.6"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"baseline-browser-mapping": "^2.9.19",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"tailwindcss": "^4",
		"tsx": "^4.20.4",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5"
	}
}
